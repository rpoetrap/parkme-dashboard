{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "next build && tsc --project tsconfig.server.json && npm run lint",
    "start": "NODE_ENV=production node dist/server.js",
    "type-check": "tsc",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "moment": "^2.27.0",
    "next": "latest",
    "query-string": "^6.13.1",
    "react": "^16.12.0",
    "react-data-table-component": "^6.11.0",
    "react-dom": "^16.12.0",
    "react-icons": "^3.10.0",
    "react-select": "^3.1.0",
    "react-svg": "^11.0.29",
    "reactstrap": "^8.5.1",
    "recharts": "^1.8.5",
    "styled-components": "^5.1.1",
    "sweetalert2": "^9.17.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/express": "^4.17.7",
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-select": "^3.0.14",
    "@types/reactstrap": "^8.5.0",
    "@types/recharts": "^1.8.14",
    "@types/styled-components": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "eslint": "^7.6.0",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "next-compose-plugins": "^2.2.0",
    "nodemon": "^2.0.4",
    "sass": "^1.26.10",
    "ts-node": "^8.10.2",
    "typescript": "3.7.3"
  },
  "license": "ISC",
  "nodemonConfig": {
    "ext": "ts, tsx, scss, json",
    "execMap": {
      "ts": "npm run lint && ts-node --project tsconfig.server.json"
    }
  }
}
