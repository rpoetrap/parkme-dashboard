{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node dist/server.js",
    "type-check": "tsc"
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "next": "latest",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-icons": "^3.10.0",
    "react-svg": "^11.0.29"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/express": "^4.17.7",
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "next-compose-plugins": "^2.2.0",
    "nodemon": "^2.0.4",
    "sass": "^1.26.10",
    "ts-node": "^8.10.2",
    "typescript": "3.7.3"
  },
  "license": "ISC",
  "nodemonConfig": {
    "ext": "ts, tsx, scss, json",
    "execMap": {
      "ts": "ts-node --project tsconfig.server.json"
    }
  }
}
