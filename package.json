{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon server/index.ts",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node dist/server.js",
    "type-check": "tsc"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "next": "latest",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "3.7.3"
  },
  "license": "ISC",
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node --project tsconfig.server.json"
    }
  }
}
